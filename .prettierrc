{
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "tailwindAttributes": [
        "imgClassName",
        "className"
    ],
    "tailwindFunctions": [
        "cn",
        "clsx",
        "cva"
    ],
    "printWidth": 80,
    "useTabs": false,
    "trailingComma": "es5",
    "bracketSameLine": false,
    "endOfLine": "lf"
}